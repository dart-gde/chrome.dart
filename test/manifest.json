{
  "name": "chrome.dart - test",
  "manifest_version": 2,  
  
  "version": "1",
  "minimum_chrome_version": "23",
  
  "default_locale": "en",
  
  "permissions": [
    "serial", {
      "fileSystem": ["write"]
    },
    "storage",
    "power",
    "pushMessaging"
  ],
  
  "app": {
    "background": {
      "scripts": ["main.js"],
      "persistent": false
    }
  }
}
